T = 0.01;
N = 100;
period = 1;
t = linspace(0,period,N);
x = linspace(0,period,N);
y = linspace(0,period,N);
z = linspace(0,period,N);
a = 0.2;
b = 0.2;
c = 5.7;
x(1) = 5;
y(1) = 5;
z(1) = 10;

xdot = -y-z;
zdot = b+z.*(x-c);

for t = 0:N
   x(t+1) = x(t)+(-y(time)-z(time)
   y(t + 1) = y(t)+(x+a.*y(t));
    
    
end

plot()